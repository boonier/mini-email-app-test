showPreviewHandler = (id) => {\n        fetchData(EMAIL_URLS[id-1])\n            .then(response => response.json())\n            .then(msg => this.setState({\n                showMessagePreview: true,\n                selectedMsg: msg\n            }));        \n    }\n\n    componentDidMount() {\n        fetchData(EMAILS_URL)\n            .then(response => response.json())\n            .then(data => this.setState({data: data}));\n    }\n\n    render() {\n        const showMessagePreview = this.state.showMessagePreview;\n        let data = this.state.data;\n        let result;\n\n        if(data) {\n            result = (\n                <Fragment>\n                    {showMessagePreview ? \n                    <MessagePreview id={this.state.previewId} data={this.state.selectedMsg} /> : \n                    <MessageList showPreviewHandler={this.showPreviewHandler} data={data} /> \n                    }\n                </Fragment>\n            );\n        } else {\n            result = null;\n        }\n        return result;\n    }\n}"},"lineCount":null}},"hash":"05f7ae874afd5818fa22578a52dddad8","cacheData":{"env":{}}}